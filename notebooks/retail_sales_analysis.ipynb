# Category-level revenue analysis
category_revenue = (
    df.groupby('category')['revenue']
      .sum()
      .sort_values(ascending=False)
)

# Plot category performance
category_revenue.plot(
    kind='bar',
    title='Revenue by Product Category'
)

plt.xlabel('Category')
plt.ylabel('Total Revenue')
plt.tight_layout()
plt.show()
